@model List<OnlineSinav.Model.Question>

@for (int i = 0; i < Model.Count; i++)
{
    <li class="list-group-item @(i == 0 ? "active" : "")">
        <a href="#">
            <span class="media">
                <span class="media-left">
                    <span class="btn btn-white btn-circle">#@Model[i].QuestionID</span>
                </span>
                <span class="media-body media-middle">
                    @Model[i].Text
                </span>
            </span>
        </a>
    </li>
}